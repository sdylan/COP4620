VARDEC ? TYPESPEC id [ num ] ;
TYPESPEC ? int
TYPESPEC ? float
TYPESPEC ? void
FUNDEC ? TYPESPEC id ( PARAMS ) COMPSTMT
PARAMS ? PARAMLIST 
PARAMS ? void
PARAMLIST ? PARAMLIST , PARAM
PARAMLIST ? PARAM
PARAM ? TYPESPEC id [ ]
PARAM ? TYPESPEC id
COMPSTMT ? { LOCDEC STMTLIST }
LOCDEC ? LOCDEC VARDEC
LOCDEC ? @
