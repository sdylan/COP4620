STMTLIST ? @
STMT ? EXPSTMT
STMT ? COMPSTMT
STMT ? SELSTMT
STMT ? ITERSTMT
STMT ? RETSTMT
EXPSTMT ? EXP ;
EXPSTMT ? ;
SELSTMT ? if ( EXP ) STMT else STMT
SELSTMT ? if ( EXP ) STMT
ITERSTMT ? while ( EXP ) STMT
RETSTMT ? return ;
RETSTMT ? return EXP ;
EXP ? VAR = EXP
EXP ? SIMPEXP
VAR ? id [ EXP ]
VAR ? id
SIMPEXP ? ADDEXP RELOP ADDEXP
SIMPEXP ? ADDEXP
RELOP ? <=
RELOP ? <
